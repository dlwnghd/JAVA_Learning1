package day08;

import java.util.Iterator;

public class ArTest2 {
	public static void main(String[] args) {
		int[] ar1 = new int[3];
		System.out.println("ar1.leng : "+ar1.length);
		for (int  i = 0;  i < ar1.length; i++) {
			System.out.println("ar1[" + i + "] : " +ar1[i]);
		}
		
		double[] ar2 = new double[3];
		System.out.println("ar2.length : " + ar2.length);
		for (int i = 0; i < ar2.length; i++) {
			System.out.println("ar2[" + i + "] : "+ ar2[i]);
		}
		
		char[] ar3 = new char [3];
		System.out.println(ar3[0]);
		
		String[] ar4 = new String [3];
		System.out.println(ar4[0]);
		
//		null : 클래스 타입을 초기화 할때 사용
		String name = null;
		
		
	}
}
