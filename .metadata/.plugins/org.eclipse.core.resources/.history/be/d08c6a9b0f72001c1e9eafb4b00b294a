package day08;

import java.util.Iterator;

public class ArTest2 {
	public static void main(String[] args) {
		int[] ar1 = new int[3];	// int배열을 ar1이라는 이름에 새로넣을 공간을 int로 3개 만들어줘라
		System.out.println("ar1.length : " + ar1.length);	// ar1의 데이터 길이 출력
		for (int i = 0; i < ar1.length; i++) {		// int값 i가 0부터; ar1의 데이터길이미만 동안; i++
			System.out.println("ar1[" + i + "] : " + ar1[i]);		// ar1[i] 출력, i = 0,1,2,3
		}

		double[] ar2 = new double[3];
		System.out.println("ar2.length : " + ar2.length);
		for (int i = 0; i < ar2.length; i++) {
			System.out.println("ar2[" + i + "] : " + ar2[i]);
		}

		char[] ar3 = new char[3];
		System.out.println(ar3[0]);

		String[] ar4 = new String[3];
		System.out.println(ar4[0]);

//		null : 클래스 타입을 초기화 할때 사용
		String name = null;
		
		//-----------------------------------------------------
//		 배열 선언방법 종류 3가지
		int [] ar5 = {10};
		int []ar6 = {10};
		int ar7[] = {10};
	}
}